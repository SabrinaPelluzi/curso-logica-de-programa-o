algoritmo "semnome"
var
   v1, v2, S : Inteiro
   
Funcao Soma (X, Y :Inteiro): Inteiro
inicio
      Retorne (X + Y)
FimFuncao

inicio
      Escreva("Digite o primeiro valor: ")
      Leia(V1)
      Escreva("Digite o segundo valor: ")
      Leia(V2)
      S <- Soma(V1, V2)
      Escreva("A soma entre ", V1, " e ", V2, "é igual ", S)
fimalgoritmo